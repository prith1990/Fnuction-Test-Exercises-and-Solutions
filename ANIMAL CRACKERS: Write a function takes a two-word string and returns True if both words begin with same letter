def animal_crackers(text):
    string1 = text.split()
    new_list = [word[0] for word in string1]
    if (new_list[0] == new_list[1]):
        return(True)
    else:
        return(False)

result = animal_crackers('Levelheaded Llama')
print(result)

result = animal_crackers('Crazy Kangaroo')
print(result)
