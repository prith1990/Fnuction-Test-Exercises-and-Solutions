def has_33(nums):
    pos = 0
    found = 'no'
    while (pos < (len(nums)-1)):
        if (nums[pos] == 3) & (nums[pos+1] == 3):
            found = 'yes'
            return True
            break
        elif ((pos+1) < (len(nums)-1)):
            pos +=1
        else:
            return False
        
        

print(has_33([1, 3, 3]))
print(has_33([1, 3, 1, 3]))
print(has_33([3, 1, 3]))
